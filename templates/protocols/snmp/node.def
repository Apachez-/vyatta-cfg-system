help: "Configure SNMP"
create: "/opt/vyatta/sbin/vyatta-snmp.pl --init-snmp"
delete: "touch /tmp/snmp.\\$PPID"
end: "if [ -f \"/tmp/snmp.\\$PPID\" ]; then \
        /opt/vyatta/sbin/vyatta-snmp.pl --stop-snmp \
        rm -rf /tmp/snmp.\\$PPID; \
      else \
        /opt/vyatta/sbin/vyatta-snmp.pl --update-snmp; \
      fi; "
