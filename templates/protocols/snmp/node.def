help: "Configure SNMP"
create: "/opt/vyatta/sbin/vyatta-snmp.pl --init-snmp"
delete: "touch /tmp/snmp.\\$PPID"
end: "if [ -f \"/tmp/snmp.\\$PPID\" ]; then \
        sudo /opt/vyatta/sbin/vyatta-snmp.pl --stop-snmp \
        rm /tmp/snmp.\\$PPID; \
      else \
        sudo /opt/vyatta/sbin/vyatta-snmp.pl --update-snmp; \
      fi; "
